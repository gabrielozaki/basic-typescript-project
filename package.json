{
  "name": "basic-typescript-project",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --declaration",
    "commit": "npx git-cz",
    "lint": "eslint ./src --ext .ts",
    "start": "ts-node ./src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:integration": "jest tests/integration/*",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.28",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/mysql": "^2.15.15",
    "@types/node": "^14.11.5",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-typescript": "^11.0.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "git-cz": "^4.7.1",
    "jest": "^26.5.2",
    "prettier": "^2.1.2",
    "testcontainers": "^4.3.0",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.63",
    "typescript": "^4.0.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabrielozaki/basic-typescript-project.git"
  },
  "author": "Gabriel Ozaki",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gabrielozaki/basic-typescript-project/issues"
  },
  "homepage": "https://github.com/gabrielozaki/basic-typescript-project#readme"
}
